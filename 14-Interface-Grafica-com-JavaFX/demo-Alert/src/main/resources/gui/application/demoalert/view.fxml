<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="383.0" prefWidth="253.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.application.demoalert.ViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="txtLabel" text="Testing Function Alerts" textAlignment="CENTER">
        <font>
            <Font size="13.0" />
        </font>
    </Label>

    <Button fx:id="buttonInformation" onAction="#onButtonInformationAction" prefHeight="37.0" prefWidth="110.0" text="Button Infor" />
    <Button fx:id="buttonError" layoutX="77.0" layoutY="203.0" onAction="#onButtonErrorAction" prefHeight="37.0" prefWidth="109.0" text="Button Error" />
    <Button fx:id="buttonConfirm" layoutX="89.0" layoutY="231.0" onAction="#onButtonConfirmAction" prefHeight="37.0" prefWidth="110.0" text="Button Confirm" />
</VBox>
